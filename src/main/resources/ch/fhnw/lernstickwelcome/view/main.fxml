<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane minHeight="486.0" minWidth="800.0" prefHeight="486.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fhnw.lernstickwelcome.fxmlcontroller.MainController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <SplitPane dividerPositions="0.3" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <items>
                  <ListView fx:id="lvMenuPane" fixedCellSize="60.0" SplitPane.resizableWithParent="false" />
                  <ScrollPane fx:id="spMainPane" id="MainPane" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" />
               </items>
            </SplitPane>
         </center>
         <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button fx:id="btSaveButton" mnemonicParsing="false" text="%welcomeApplicationStart.saveButton" />
                <Button fx:id="btFinishButton" mnemonicParsing="false" text="%welcomeApplicationStart.finishButton" />
              </buttons>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
               </BorderPane.margin>
            </ButtonBar>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
