    <!-- nonfree (utility) apps -->

    <application name="Adobe Reader">
        <description>Adobe_Reader_Description</description>
        <icon>Adobe_Reader_8_icon</icon>
        <help-path>Adobe_Reader_Website</help-path>
        <installed-names>
            <installed-dpkg-name>adobereader-enu</installed-dpkg-name>
        </installed-names>
        <packages>
            <!--
            Dependencies explained:

            libgtk2.0-0:i386:
            hard dependency of adobereader-enu

            libatk1.0-0:i386:
            hard dependency of libgtk2.0-0:i386

            libgail-common:i386
            otherwise acroread throws this error message at startup:
            Gtk-Message: 16:26:36.258: Failed to load module "gail"

            libatk-adaptor:i386
            otherwise acroread throws this error message at startup:
            Gtk-Message: 16:26:36.259: Failed to load module "atk-bridge"

            gtk2-engines-murrine:i386
            otherwise acroread throws this error message at startup:
            Unable to locate theme engine in module_path: "murrine"

            gtk2-engines-pixbuf:i386
            otherwise acroread throws this error message at startup:
            Unable to locate theme engine in module_path: "pixmap"
            -->
            <package type="apt">gtk2-engines-murrine:i386</package>
            <package type="apt">gtk2-engines-pixbuf:i386</package>
            <package type="apt">libatk1.0-0:i386</package>
            <package type="apt">libatk-adaptor:i386</package>
            <package type="apt">libgail-common:i386</package>
            <package type="apt">libgtk2.0-0:i386</package>
            <package type="wget" fetchUrl="http://ardownload.adobe.com/pub/adobe/reader/unix/9.x/9.5.5/enu/{0}" saveDir="USER_HOME">AdbeRdr9.5.5-1_i386linux_enu.deb</package>
            <package type="apt">lernstick-adobereader-enu</package>
        </packages>
        <tags>
            <tag>utility</tag>
        </tags>
    </application>

