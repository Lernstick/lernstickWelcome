                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="3.0" prefWidth="598.0" />
              </content>
            </TitledPane>
         </top>
         <center>
            <SplitPane fx:id="SplitPane" dividerPositions="0.27759197324414714" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <items>
                  <VBox fx:id="MenuPane" prefHeight="310.0" prefWidth="99.0" />
                  <Pane fx:id="MainPane" prefHeight="310.0" prefWidth="428.0" />
               </items>
            </SplitPane>
         </center>
         <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button fx:id="SaveButton" mnemonicParsing="false" onMouseClicked="#onSaveClickedAction" text="%welcomeApplicationStart.saveButton" />
                  <Button fx:id="FinishButton" mnemonicParsing="false" onMouseClicked="#onFinishClickedAction" text="%welcomeApplicationStart.finishButton" />
