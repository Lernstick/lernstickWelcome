                      <TableColumn fx:id="tab_fw_server_protocol" prefWidth="66.0" text="%welcomeApplicationFirewall.protocol" />
                      <TableColumn fx:id="tab_fw_server_ip" prefWidth="121.0" text="%welcomeApplicationFirewall.ip" />
                        <TableColumn fx:id="tab_fw_server_port" prefWidth="86.0" text="%welcomeApplicationFirewall.port" />
                        <TableColumn fx:id="tab_fw_server_desc" prefWidth="174.0" text="%welcomeApplicationFirewall.serverDescription" />
                    </columns>
                  </TableView>
                  <Pane prefHeight="73.0" prefWidth="449.0">
