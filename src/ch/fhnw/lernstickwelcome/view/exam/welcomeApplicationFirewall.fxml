                      <TableColumn fx:id="tab_fw_allowed_sites" prefWidth="131.0" text="%welcomeApplicationFirewall.searchCriteria" />
                      <TableColumn fx:id="tab_fw_allowed_sites_firewall" prefWidth="317.0" text="%welcomeApplicationFirewall.searchPattern" />
